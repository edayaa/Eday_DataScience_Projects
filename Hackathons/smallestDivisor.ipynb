    "def smallestDiv(n): \n",
    "    if (n % 2 == 0): \n",
    "        return 2\n",
    "   \n",
    "    i = 3\n",
    "    while(i * i <= n): \n",
    "        if (n % i == 0): \n",
    "            return i\n",
    "        i += 2\n",
    "    return n  "
   ]
